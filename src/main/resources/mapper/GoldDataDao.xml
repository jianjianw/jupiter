<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qiein.jupiter.web.dao.GoldDataDao">
    <resultMap id="goldData" type="com.qiein.jupiter.web.entity.po.GoldFingerPO">
        <id column="ID" jdbcType="INTEGER" property="id"/>
        <result column="FORMID" jdbcType="VARCHAR" property="formId"/>
        <result column="FORMNAME" jdbcType="VARCHAR" property="formName"/>
        <result column="KZNAMEFIELD" jdbcType="VARCHAR" property="kzNameField"/>
        <result column="KZPHONEFIELD" jdbcType="VARCHAR" property="kzPhoneField"/>
        <result column="KZWECHATFIELD" jdbcType="VARCHAR" property="kzWechatField"/>
        <result column="FIELDKEY" jdbcType="VARCHAR" property="fieldKey"/>
        <result column="FIELDVALUE" jdbcType="VARCHAR" property="fieldValue"/>
        <result column="SRCID" jdbcType="INTEGER" property="srcId"/>
        <result column="SRCNAME" jdbcType="VARCHAR" property="srcName"/>
        <result column="TYPEID" jdbcType="INTEGER" property="typeId"/>
        <result column="TYPENAME" jdbcType="VARCHAR" property="typeName"/>
        <result column="ADID" jdbcType="INTEGER" property="adId"/>
        <result column="ADADDRESS" jdbcType="VARCHAR" property="adAddress"/>
        <result column="ZXSTYLE" jdbcType="VARCHAR" property="zxStyle"/>
        <result column="MEMO" jdbcType="VARCHAR" property="memo"/>
        <result column="ISSHOW" jdbcType="INTEGER" property="isShow"/>
        <result column="POSTURL" jdbcType="VARCHAR" property="postURL"/>
        <result column="CREATEORID" jdbcType="INTEGER" property="createorId"></result>
        <result column="CREATEORNAME" jdbcType="VARCHAR" property="createorName"/>
        <result column="CREATETIME" jdbcType="INTEGER" property="createTime"/>
        <result column="AREALIMIT" jdbcType="INTEGER" property="areaLimit"/>
        <result column="ISFILTER" jdbcType="INTEGER" property="isFilter"/>
        <result column="PUSHNEWS" jdbcType="INTEGER" property="pushNews"/>
        <result column="COMPANYID" jdbcType="INTEGER" property="companyId"></result>
    </resultMap>
    <resultMap id="goldCustomer" type="com.qiein.jupiter.web.entity.vo.GoldCustomerVO">
        <id column="ID" jdbcType="INTEGER" property="id"/>
        <result column="FORMID" jdbcType="VARCHAR" property="formId"/>
        <result column="FORMNAME" jdbcType="VARCHAR" property="formName"/>
        <result column="SRCID" jdbcType="INTEGER" property="srcId"/>
        <result column="SRCNAME" jdbcType="VARCHAR" property="srcName"/>
        <result column="TYPEID" jdbcType="INTEGER" property="typeId"/>
        <result column="TYPENAME" jdbcType="VARCHAR" property="typeName"/>
        <result column="MEMO" jdbcType="VARCHAR" property="memo"/>
        <result column="CREATETIME" jdbcType="INTEGER" property="createTime"/>
        <result column="ADID" jdbcType="INTEGER" property="adId"/>
        <result column="ADADDRESS" jdbcType="VARCHAR" property="adAddress"/>
        <result column="KZNAME" jdbcType="VARCHAR" property="kzName"/>
        <result column="KZPHONE" jdbcType="VARCHAR" property="kzPhone"/>
        <result column="WECHAT" jdbcType="VARCHAR" property="wechat"/>
        <result column="ADDRESS" jdbcType="VARCHAR" property="address"/>
        <result column="REMARK" jdbcType="VARCHAR" property="remark"/>
        <result column="IP" jdbcType="VARCHAR" property="ip"/>
        <result column="IPADDRESS" jdbcType="VARCHAR" property="ipAddress"/>
        <result column="COMPANYID" jdbcType="INTEGER" property="companyId"/>
        <result column="STATUSID" jdbcType="INTEGER" property="statusId"/>
    </resultMap>
    <sql id="goldCustomerlist">
        ID, FORMID, FORMNAME, SRCNAME, TYPENAME, MEMO, FROM_UNIXTIME(CREATETIME, '%Y/%m/%d %H:%i') createTime, KZNAME, KZPHONE, WECHAT, ADDRESS, REMARK, IP, IPADDRESS, STATUSID
    </sql>
    <sql id="formInsert">
        FORMID, FORMNAME, KZNAMEFIELD, KZPHONEFIELD, KZWECHATFIELD, FIELDKEY, FIELDVALUE, SRCID, SRCNAME, TYPEID, TYPENAME, ADID, ADADDRESS, ZXSTYLE, MEMO, ISSHOW, POSTURL, CREATEORID, CREATEORNAME, CREATETIME, COMPANYID, ISFILTER, PUSHNEWS
    </sql>
    <sql id="formList">
        ID, FORMID, FORMNAME, KZNAMEFIELD, KZPHONEFIELD, KZWECHATFIELD, FIELDKEY, FIELDVALUE, SRCID, SRCNAME, TYPEID, TYPENAME, ADID, ADADDRESS, ZXSTYLE, MEMO, ISSHOW, POSTURL, CREATEORID,CREATEORNAME, FROM_UNIXTIME(CREATETIME, '%Y/%m/%d %H:%i') createTime, AREALIMIT, ISFILTER, PUSHNEWS,COMPANYID
    </sql>
    <sql id="formUpdate">
        FORMID=#{formId}, FORMNAME=#{formName}, KZNAMEFIELD=#{kzNameField}, KZPHONEFIELD=#{kzPhoneField}, KZWECHATFIELD=#{kzWechatField}, FIELDKEY=#{fieldKey}, FIELDVALUE=#{fieldValue}, SRCID=#{srcId}, SRCNAME=#{srcName}, TYPEID=#{typeId}, TYPENAME=#{typeName}, ADID=#{adId}, ADADDRESS=#{adAddress}, ZXSTYLE=#{zxStyle}, MEMO=#{memo}, POSTURL=#{postURL}, ISSHOW=#{isShow}, AREALIMIT=#{areaLimit}, ISFILTER=#{isFilter}, PUSHNEWS=#{pushNews}
    </sql>
    <insert id="insert">
        INSERT INTO hm_crm_gold_plug_setting (
        <include refid="formInsert"/>
        )
        VALUES
        (
        #{formId},
        #{formName},
        #{kzNameField},
        #{kzPhoneField},
        #{kzWechatField},
        #{fieldKey},
        #{fieldValue},
        #{srcId},
        #{srcName},
        #{typeId},
        #{typeName},
        #{adId},
        #{adAddress},
        #{zxStyle},
        #{memo},
        #{isShow},
        #{postURL},
        #{staffId},
        #{createorName},
        UNIX_TIMESTAMP(now()),
        #{companyId},
        #{isFilter},
        #{pushNews}
        )
    </insert>

    <delete id="delete">
        delete from hm_crm_gold_plug_setting
        where id = #{id}
    </delete>


    <update id="update">
        update hm_crm_gold_plug_setting set
        <include refid="formUpdate"/>
        where id=#{id}
    </update>

    <select id="select" resultMap="goldData">
        select
        <include refid="formList"/>
        from hm_crm_gold_plug_setting where companyId=#{companyId} order by id DESC

    </select>

    <update id="editOpenOrClose">
        update hm_crm_gold_plug_setting
        <set>
            <if test="isShow!=null">
                ISSHOW=#{isShow},
            </if>
            <if test="areaLimit!=null">
                AREALIMIT=#{areaLimit},
            </if>
            <if test="isFilter!=null">
                ISFILTER=#{isFilter},
            </if>
            <if test="pushNews!=null">
                PUSHNEWS=#{pushNews},
            </if>
        </set>
        where id=#{id}

    </update>

    <select id="goldCustomerSelect" resultMap="goldCustomer">
        select
        <include refid="goldCustomerlist"/>
        from hm_crm_gold_temp where formId=#{formId}
        <if test="startTime!=null and startTime!=''">
            and createTime &gt;= #{startTime}
        </if>
        <if test="endTime!=null and endTime!=''">
            and createTime &lt;= #{endTime}
        </if>
        <if test="statusId!=null and statusId!=''">
            and statusId =#{statusId}
        </if>
        order by id DESC
    </select>

    <select id="checkForm" resultMap="goldData">
        select id from hm_crm_gold_plug_setting where (FORMID=#{formId} OR FORMNAME=#{formName})
        <if test=" id !=null">
            and id !=#{id}
        </if>
    </select>

    <select id="findForm" resultMap="goldData">
        select
        <include refid="formList"/>
        from hm_crm_gold_plug_setting where formId=#{formId} limit 0,1
    </select>

    <!--获取金数据表单-->
    <select id="getGoldFingerByFormIdAndFormName" resultMap="goldData">
        select
        <include refid="formList"/>
        from hm_crm_gold_plug_setting where formId = #{formId} and formName = #{formName}
    </select>
</mapper>